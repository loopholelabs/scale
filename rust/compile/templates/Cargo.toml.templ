[package]
name = "compile"
version = "0.1.0"
edition = "2021"

[dependencies]
wee_alloc = "0.4.5"
{{ range $dependency := .dependencies -}}
{{ $dependency.Name }} = "{{ $dependency.Version }}"
{{end -}}

{{ if .signature_path }}
[dependencies.{{ .signature }}]
path = "{{ .signature_path }}"
{{ end }}

[lib]
crate-type = ["cdylib"]
path = "lib.rs"

[profile.release]
opt-level = "z"
lto = true
codegen-units = 1
